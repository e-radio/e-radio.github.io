/* Player */
.audio-player {
    background: linear-gradient(to bottom, rgba(0, 0, 0, 0) 10%, rgba(0, 0, 0, 0.7));
    z-index: 1;
    position: relative;
}

.audio-player .slider-progress {
    position: relative;
    width: 100%;
    height: 4px;
}

.audio-player .slider-progress .player-progress-input {
    position: absolute;
    z-index: 10;
    top: 0;
    bottom: 0;
    width: 100%;
    cursor: pointer;
    opacity: 0;
}

.audio-player .slider-progress .player-bar {
    position: absolute;
    z-index: 1;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(255, 255, 255, 0.4);
}

.audio-player .slider-progress .player-progress {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    background-color: var(--white);
}

/*.audio-player.live .slider-progress .player-progress {
    width: 100% !important;
}
.audio-player.live.playing .slider-progress .player-progress-input {
    cursor: default;
}
*/


.audio-player:not(.live) .time .live-streaming,
.audio-player:not(.live) .start-button,
.audio-player:not(.live) .live-button,
.audio-player:not(.live) .pop-out-button,
.audio-player.live .back-button,
.audio-player.live .end-button {
    display: none;
}

/*
.audio-player.live .time .live-streaming,
.audio-player.live .pop-out-button {
    display: block;
}

.audio-player.live .time .total-time,
.audio-player.live .time .live-streaming {
    margin-top: 0.25rem;
}
*/

.audio-player .slider-progress .player-dot {
    position: absolute;
    background-color: var(--white);
    right: 0;
    top: -4px;
    border-radius: 100%;
    width: 12px;
    height: 12px;
}


.audio-player .slider-volume {
    position: relative;
    width: 100px;
    height: 4px;
    margin-left: 8px;
}

.audio-player .slider-volume .volume-progress-input {
    position: absolute;
    z-index: 10;
    bottom: -9px;
    height: 24px;
    width: 104%;
    right: -2%;
    left: -2%;
    cursor: pointer;
    opacity: 0;
}

.audio-player .slider-volume .volume-bar {
    position: absolute;
    z-index: 1;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(255, 255, 255, 0.4);
}

.audio-player .slider-volume .volume-progress {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    background-color: var(--white);
    width: var(--player-volume);
}


.audio-player .slider-volume .volume-dot {
    position: absolute;
    background-color: var(--white);
    left: 100%;
    top: -4px;
    border-radius: 100%;
    width: 12px;
    height: 12px;
}


.audio-player .play-button .play {
    display: block;
}

.audio-player .play-button .pause {
    display: none;
}

.audio-player.playing .play-button .play {
    display: none;
}

.audio-player.playing .play-button .pause {
    display: block;
}

.audio-player .volume-button .mute {
    display: none;
}

.audio-player .volume-button .volume {
    display: block;
}

.audio-player.muted .volume-button .mute {
    display: block;
}

.audio-player.muted .volume-button .volume {
    display: none;
}

/*.audio-player.live .back-button,
.audio-player.live .backward-button,
.audio-player.live .forward-button,
.audio-player.live .live-button {
    pointer-events: none;
    opacity: 0.5;
}*/

.audio-player .controls button {
    background-color: transparent;
    color: var(--white);
    padding: 0;
    margin: 0 4px;
    min-width: 40px;
    min-height: 40px;
}

.audio-player .controls button:hover {
    color: var(--accent);
}


.audio-player .controls button svg {
    vertical-align: middle;
}

.audio-player .controls button:hover svg,
.audio-player .controls button:active svg {
    fill: var(--accent);
}

.audio-player .live-streaming svg {
    margin-right: 0.25rem;
}

.audio-player.live .time .live-streaming svg {
    fill: var(--white);
}

.audio-player.playing .live-pulsate {
    animation: 1.7s ease infinite pulsate;
    fill: #e91e63;
}

.audio-player .controls > * {
    width: auto;
}

.audio-player .controls svg {
    width: 3em;
    height: 3em;
}

.audio-player .controls .volume-button svg {
    width: 2em;
    height: 2em;
}

.audio-player:not(.mini-player) .controls .play-button svg {
    width: 4.5em;
    height: 4.5em;
}

.audio-player .controls .pop-out-button svg {
    width: 1.8em;
    height: 1.8em;
}

.audio-player .controls > *:first-child,
.audio-player .controls > *:last-child {
    width: 160px;
}

@media screen and  (min-width: 960px) {
    .audio-player .controls svg {
        font-size: 18px;
    }
}

@media screen and  (max-width: 959px) {
    .audio-player .controls svg {
        font-size: 16px;
    }
}


.audio-player .loading-wrapper {
    display: none;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    flex-direction: row;
    box-sizing: border-box;
    place-content: center;
    align-items: center;
    z-index: 1;
}

.audio-player .loading-wrapper div {
    display: inline-block;
    margin: 0 4px;
    width: 4px;
    background: var(--accent);
    -webkit-animation: infinite-loading 0.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
    animation: infinite-loading 1.2s cubic-bezier(0, 0.5, 0.5, 1) infinite;
}

.audio-player .loading-wrapper div:nth-child(5),
.audio-player .loading-wrapper div:nth-child(1) {
    -webkit-animation: infinite-loading-small 0.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
    animation: infinite-loading-small 1.2s cubic-bezier(0, 0.5, 0.5, 1) infinite;
}

.audio-player .loading-wrapper div:nth-child(5) {
    animation-delay: 0s;
}

.audio-player .loading-wrapper div:nth-child(4) {
    animation-delay: -0.24s;
}

.audio-player .loading-wrapper div:nth-child(3) {
    animation-delay: -0.12s;
}

.audio-player .loading-wrapper div:nth-child(2) {
    animation-delay: -0.36s;
}

.audio-player .loading-wrapper div:nth-child(1) {
    animation-delay: -0.48s;
}

.audio-player.loading .loading-wrapper {
    display: flex;
}

.toggle-players {
    float: right;
    margin-bottom: 0.5rem;
}

@media screen and (min-width: 960px) {

    .toggle-players {
        position: absolute;
        top: 0;
        right: 0;
        z-index: 2;
    }
}

.toggle-players svg {
    width: 48px;
    height: 48px;
    vertical-align: middle;
    -moz-transition: all 0.35s ease-in-out 0s;
    -o-transition: all 0.35s ease-in-out 0s;
    -webkit-transition: all 0.35s ease-in-out 0s;
    transition: all 0.35s ease-in-out 0s;
}


.toggle-players .audio-streaming {
    display: none;
}


.pop-out .controls svg {
    font-size: 12px;
}

.enable-video .toggle-players .audio-streaming {
    display: block;
}

button .video-streaming {
    -webkit-animation: heartbeat 1.5s ease-in-out infinite both;
    animation: heartbeat 1.5s ease-in-out infinite both;
    display: block;
}

.enable-video .toggle-players .video-streaming {
    display: none;
}

.video-player-wrapper {
    display: none;
}

.audio-player-wrapper {
    display: block;
}

.enable-video .video-player-wrapper {
    display: block;
}

.enable-video .audio-player-wrapper {
    display: none;
}

/* MODAL */

.modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    z-index: 100002;
    display: none;
}

.modal.close,
.modal.open {
    display: flex !important;
}

.modal.open .modal-backdrop {
    -webkit-animation: fade-in 1.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
    animation: fade-in 1.2s cubic-bezier(0.390, 0.575, 0.565, 1.000) both;
}

.modal.open .modal-wrapper {
    -webkit-animation: slide-fade-top 0.25s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
    animation: slide-fade-top 0.25s cubic-bezier(0.250, 0.460, 0.450, 0.940) both;
}

.modal.close .modal-backdrop {
    -webkit-animation: fade-out 0.25s ease-out both;
    animation: fade-out 0.25s ease-out both;
}

.modal.close .modal-wrapper {
    -webkit-animation: slide-out-bottom 0.5s cubic-bezier(0.550, 0.085, 0.680, 0.530) both;
    animation: slide-out-bottom 0.5s cubic-bezier(0.550, 0.085, 0.680, 0.530) both;
}

.modal-backdrop {
    z-index: 1;
    background-color: rgba(0, 0, 0, 0.75);
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

.modal-wrapper {
    z-index: 2;
    background-color: var(--white);
    width: 100%;
}

.react-modal .modal-wrapper .column {
    width: 100%;
}

.react-modal .modal-wrapper {
    max-width: 370px;
    width: 100%;
}


