      // 1) Δοκίμασε αμέσως
      tryPlay();

      // 2) Αν ο χρήστης αλληλεπιδράσει με τη σελίδα, ξαναδοκίμασε (μερικοί browsers επιτρέπουν τότε).
      ['click','touchend','keydown'].forEach(function(evt){
        window.addEventListener(evt, function once(){
          tryPlay(); window.removeEventListener(evt, once, {once:true});
        }, {once:true});
      });

      // 3) Χειροκίνητο κουμπί
      if (startBtn) {
        startBtn.addEventListener('click', function(){
          audio.play().then(function(){ fallback.classList.add('hidden'); });
        });
      }
    })();
  </script>
