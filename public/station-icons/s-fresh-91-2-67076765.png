      function tryPlay(){
        if (!audio) return;
        var p = audio.play();
        if (p && typeof p.catch === 'function') {
          p.catch(function(){
            // Autoplay blocked -> δείξε fallback κουμπί
            fallback.classList.remove('hidden');
