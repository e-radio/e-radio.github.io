        <script>
        document.addEventListener("DOMContentLoaded", () => {
            const onAirChangeAt = $('[data-on-air-change-at]').data('on-air-change-at');
            const programmeChangeAt = $('[data-programme-change-at]').map(function() {
                return $(this).data('programme-change-at');
            }).get();

            let lastTriggeredOnAir = null;
            let lastTriggeredProgramme = null;

            setInterval(() => {
                const now = new Date();
                const hours = String(now.getHours()).padStart(2, '0');
                const minutes = String(now.getMinutes()).padStart(2, '0');
                const currentTime = `${hours}:${minutes}`;

                changeOnAir(currentTime);
                changeProgramme(currentTime);
            }, 1000);

            function changeOnAir(currentTime) {
                if (onAirChangeAt === currentTime && lastTriggeredOnAir !== currentTime) {
                    lastTriggeredOnAir = currentTime;
                    $.ajax({
                        url: '/get-up-next-programme',
                        headers: {
                            "Change-Current-Programme": true,
                            "For-Programme": "On Air"
                        },
                        success: function(data) {
                            $('#on-air-container').html(data.html);
                        }
                    });
                }
            }

            function changeProgramme(currentTime) {
                if (programmeChangeAt.includes(currentTime) && lastTriggeredProgramme !== currentTime) {
                    lastTriggeredProgramme = currentTime;
                    $.ajax({
                        url: '/get-up-next-programme',
                        headers: {
                            "Change-Current-Programme": true,
                            "For-Programme": "Programme"
                        },
                        success: function(data) {
