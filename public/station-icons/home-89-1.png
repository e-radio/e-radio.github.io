    <script>
        document.addEventListener('DOMContentLoaded', function() {
            function changeGlobalProgramme() {
                const now = new Date();
                const minutes = now.getMinutes();
                const seconds = now.getSeconds();

                if (minutes === 0 && seconds === 0) {
                    $.ajax({
                        url: '/get-up-next-programme',
                        headers: {
                            "Change-Current-Programme": true,
                            "For-Programme": "Radio"
                        },
                        success: function(data) {
                            $('#radio-container').html(data.html);
                        }
                    });
                }
            }

            setInterval(changeGlobalProgramme, 1000);
        })
    </script>

            
